{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	
    "name": "g4rf-eventkrake/events-list",
	"version": "0.1.0",
	
    "title": "Eventkrake Events list",
	"description": "Shows events in a list.",
	"example": {},
    
    "category": "design",
    
    "attributes": {		
        "showContent": {
            "type": "boolean",
            "default": true
        }
    },
	
    "supports": {
        "html": false,
        "anchor": true,
        "color": {
            "background": true,
            "gradients": true,
            "link": true,
            "text": true
        },
        "spacing": {
            "margin": true,
            "padding": true
        },
        "__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		}
	},
	
    "textdomain": "g4rf-eventkrake",
    
	"editorScript": "file:./index.js",
    "viewScript": "file:./view.js",
	"style": "file:./style-index.css"
}
